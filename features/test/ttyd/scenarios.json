{
    "ttyd-default": {
        "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
        "features": {
            "ttyd": {}
        }
    },
    "ttyd-specific-version": {
        "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
        "features": {
            "ttyd": {
                "version": "1.7.7"
            }
        }
    },
    "ttyd-custom-port": {
        "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
        "features": {
            "ttyd": {
                "port": "8080"
            }
        }
    }
}
