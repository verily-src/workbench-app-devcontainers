{
  "name": "Gradio app",
  "dockerComposeFile": "docker-compose.yaml",
  "service": "app",
  "forwardPorts": [8080],
  "workspaceFolder": "/workspace",
  "postCreateCommand": "./startupscript/post-startup.sh && chmod +x startupscript/gradio.sh && ./startupscript/gradio.sh && python verily1/tools/mlinfra/langchain/app_demo/main.py --gradio_remote",
  "postStartCommand": "python verily1/tools/mlinfra/langchain/app_demo/main.py --gradio_remote",
  "remoteUser": "root",
  "customizations": {
    "vscode": {
      "extensions": ["ms-python.python"]
    }
  },
  "features": {
    "ghcr.io/dhoeric/features/google-cloud-cli:1": {}
  }
}
