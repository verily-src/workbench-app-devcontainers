{
  "name": "Jupyter app running as feature on pytorch image",
  "dockerComposeFile": "docker-compose.yaml",
  "service": "app",
  "shutdownAction": "none",
  "workspaceFolder": "/workspace",
  "postCreateCommand": [
    "/Users/rogerwangcs/local_dev/dev/startupscript/post-startup.sh",
    "jupyter",
    "/home/jupyter",
    "${templateOption:cloud}",
    "${templateOption:login}"
  ],
  "features": {
    "ghcr.io/devcontainers/features/common-utils": {
      // See https://github.com/devcontainers/features/tree/main/src/common-utils
      "installZsh": false,
      "installOhMyZsh": false,
      "installOhMyZshConfig": false,
      "username": "jupyter"
    },
    "ghcr.io/devcontainers/features/java:1": {
      "version": "17"
    },
    "ghcr.io/dhoeric/features/google-cloud-cli:1": {},
    "./.devcontainer/features/jupyter": {
      "installJupyterlab": true,
      "cloudPlatform": "${templateOption:cloud}",
      "configureJupyterlabAllowOrigin": "*"
    }
  },
  "remoteUser": "root",
  "customizations": {
    "workbench": {
      "opens": {
        "extensions": [
          // Source
          ".ipynb",
          ".R",
          ".py",
          // Documents
          ".md",
          ".html",
          ".latex",
          ".pdf",
          // Images
          ".bmp",
          ".gif",
          ".jpeg",
          ".jpg",
          ".png",
          ".svg",
          // Data
          ".csv",
          ".tsv",
          ".json",
          ".vl"
        ],
        "fileUrlSuffix": "/lab/tree/{path}",
        "folderUrlSuffix": "/lab/tree/{path}"
      }
    }
  }
}
